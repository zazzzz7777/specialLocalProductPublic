<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cl.dao.SystemNoticeDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.cl.entity.SystemNoticeEntity" id="systemNoticeMap">
        <result property="content" column="content"/>
    </resultMap>

    <select id="selectListView"
		resultType="com.cl.entity.view.SystemNoticeView" >

		SELECT  system_notice.* FROM system_notice  system_notice 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.cl.entity.view.SystemNoticeView" >
		SELECT * FROM system_notice  system_notice <where> 1=1 ${ew.sqlSegment}</where>
	</select>



</mapper>
